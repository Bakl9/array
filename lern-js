var car = ['audi', 'bmw', 'bentley']
indexOf('bentley') // метод оператор индексации элемента в масвиве 

car.push('mersedes') // метод добавление в конце массива
car.pop() // метод удаление в конце массива. 
   
array.shift() // для удаления в начале масиви    
car.unshift('lexus') // для добавления         
  
listNumber.splice(0,2); // для замены жлементов в массиве в новый массив
arr.splice (0,0,'Heroo', 'Vesna'); для добавления и удаления в массива
       
name.split(',', 2); // метод соеденяет строчные элементы.  и добавляет в новый массив, также можно ограничеть количество элементов которые будут добавляться 
array = name.join(', '); медот для приобразование массива в строку и добавляет запятую перед каждым элементом.



array.filter(word); // метод для фиьтрации массива, дополнительно для данного массива используеться цикл или стрелочная функция 
filter(world)list => list > 6)
console.log(array.revers() // сортирует масси в обратном порядке 
array.sort(compare) // метод для сортировки, для данного метода есть дополнительные функции // (return a - b)
console.log(array.sort((a,b) => a - b)) 

















